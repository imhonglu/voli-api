{
  "[html][yaml][javascript][javascriptreact][typescript][typescriptreact][json][jsonc][markdown]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": "explicit"
  },
  "files.associations": {
    "*.json": "jsonc"
  },
  "search.exclude": {
    "**/dist": true,
    "**/docs": true,
    "**/temp": true
  }
}
